{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "article-route": {
        "ClusterId": "article-cluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [{ "PathPattern": "{**catch-all}" }]
      }
    },
    "Clusters": {
      "article-cluster": {
        "LoadBalancingPolicy": "LeastRequests",
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:6001"
          }
        }
      }
    }
  }
}
